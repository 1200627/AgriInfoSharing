<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" prefHeight="825.0" prefWidth="1000.0" style="-fx-font-family: Consolas;" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jp.kujira.ais.main.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="825.0" prefWidth="1000.0">
         <bottom>
            <VBox prefHeight="600.0" prefWidth="570.0">
               <children>
                  <HBox>
                     <children>
                        <TableView fx:id="providersTable" prefHeight="200.0" prefWidth="500.0">
                          <columns>
                            <TableColumn fx:id="providerIDColumn" prefWidth="37.0" text="ID" />
                              <TableColumn fx:id="khPointerColumn" prefWidth="67.0" text="KHPointer" />
                            <TableColumn fx:id="canRequireColumn" prefWidth="72.0" text="canRequire" />
                              <TableColumn fx:id="earnedIncentiveColumn" prefWidth="109.0" text="earnedIncentive" />
                              <TableColumn fx:id="contributionColumn" prefWidth="91.0" text="contribution" />
                              <TableColumn fx:id="userButtonColumn" prefWidth="250.0" />
                          </columns>
                        </TableView>
                        <LineChart fx:id="knowHowEvaluationChart" prefHeight="149.0" prefWidth="500.0">
                          <xAxis>
                            <CategoryAxis side="BOTTOM" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis side="LEFT" upperBound="30.0" />
                          </yAxis>
                        </LineChart>
                     </children>
                  </HBox>
                  <TableView fx:id="knowHowTable" prefHeight="200.0" prefWidth="570.0">
                    <columns>
                      <TableColumn fx:id="evaluationColumn" prefWidth="78.0" text="evaluation" />
                      <TableColumn fx:id="rarityColumn" prefWidth="63.0" text="rarity" />
                        <TableColumn fx:id="usageCountColumn" prefWidth="75.0" text="usageCount" />
                        <TableColumn fx:id="diligentUsageCountColumn" prefWidth="81.0" text="dUsageCount" />
                        <TableColumn fx:id="freshnessColumn" prefWidth="75.0" text="freshness" />
                        <TableColumn fx:id="providerWorthColumn" prefWidth="91.0" text="providerWorth" />
                        <TableColumn fx:id="hiddenWorthColumn" prefWidth="89.0" text="hiddenWorth" />
                        <TableColumn fx:id="userWorthColumn" prefWidth="83.0" text="userWorth" />
                        <TableColumn fx:id="diligentUserWorthColumn" prefWidth="75.0" text="dUserWorth" />
                        <TableColumn fx:id="isConsumedColumn" prefWidth="75.0" text="isConsumed" />
                        <TableColumn fx:id="knowHowButtonColumn" prefWidth="75.0" />
                    </columns>
                  </TableView>
                  <TableView fx:id="propertiesTable" minHeight="-Infinity" prefHeight="200.0" prefWidth="330.0" BorderPane.alignment="CENTER">
                    <columns>
                      <TableColumn fx:id="propertyNameColumn" prefWidth="299.0" text="Property Name" />
                      <TableColumn fx:id="valueColumn" minWidth="0.0" prefWidth="300.0" text="Value" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
         </bottom>
         <top>
            <VBox alignment="CENTER" prefHeight="225.0" prefWidth="330.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER">
                     <children>
                        <Button mnemonicParsing="false" onAction="#onStart" text="Start" BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </BorderPane.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#onStop" text="Stop" />
                        <Button mnemonicParsing="false" onAction="#onReset" text="Reset" />
                     </children>
                  </HBox>
                  <TextArea fx:id="progressArea" editable="false" minHeight="-Infinity" onMouseClicked="#onProgressAreaDown" prefHeight="200.0" prefWidth="200.0">
                     <font>
                        <Font name="Consolas" size="12.0" />
                     </font></TextArea>
               </children>
            </VBox>
         </top>
      </BorderPane>
   </children>
</GridPane>
